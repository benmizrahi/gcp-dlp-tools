    "version": "0.2.0",
    "configurations": [

        {
            "name": "find-policy-tags",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "find-policy-tags",
                "--scan-path",
                "gcp://<org-id>/",
                "--output-path",
                "bq://<project-id>/<dataset-id>/dlp_script_output_folder"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "get-sample-scan-size",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "get-sample-scan-size",
                "--scan-path",
                "gcp://<org-id>/178230636913/<project-id>",
                "--output-path",
                "bq://<project-id>/<dataset-id>/tables_results"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "sdp_scan",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "sdp-scan",
                "--project-id",
                "<project-id>",
                "--dataset-id",
                "<dataset-id>",
                "--filter-path",
                "gcp://<org-id>/<project-id>/data/",
                "--output-path",
                "bq://<project-id>/<dataset-id>/dlp-results",
                "--info-types",
                "./infoTypes.txt"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}